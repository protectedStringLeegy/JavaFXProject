<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="itemGridPane" hgap="3.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" />
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
  </rowConstraints>
   <children>
      <Label fx:id="senderLabel" text="Label">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="subjectLabel" text="Label" GridPane.columnSpan="2" GridPane.rowIndex="1" />
      <Label fx:id="previewLabel" text="Label" wrapText="true" GridPane.columnSpan="2" GridPane.rowIndex="2">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></Label>
      <Label fx:id="dateLabel" text="Label" textFill="#00000099" GridPane.columnIndex="1" GridPane.halignment="RIGHT" />
   </children>
   <padding>
      <Insets bottom="1.0" top="1.0" />
   </padding>
</GridPane>
